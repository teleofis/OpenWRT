config smscontrol 'common'
        option enabled '0'

config call
        option enabled '0'
        option ack '0'
        option command 'reboot'

config remote
        option enabled '0'
        option received 'reboot'
        option command 'reboot'

config remote
        option enabled '0'
        option received 'factory'
        option command 'echo y | firstboot && sleep 30 && reboot'
